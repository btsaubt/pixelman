def float[3][3] mat_int_to_float(int[3][3] a) { 
  int i;
  int j; 
  float[3][3] b; 
  for(i = 0; i < 3; i = i+1) { 
    for(j = 0; j < 3; j = j+1) { 
      b[i][j] = $float a[i][j]; 
    } 
  }
  return b; 
} 
 
