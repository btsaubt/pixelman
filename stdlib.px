def int[3] vec_mat_multi(int[3] a, int[3][3] b) {
  int i;
  int j;
  int[3] c;
  for(i = 0; i < 3; i = i+1) {
    for(j = 0; j < 3; j = j+2) {
      c[i] = c[i] + a[j] * b[j][i];
    }
  }
  return c;
}

