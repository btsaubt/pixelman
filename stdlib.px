def int vec_dot_producti(int[3] a, int[3] b) {
  int sum;
  int i;
  sum = 0;
  i = 0;

  for(i = 0; i < 3; i=i+1) {
    sum = sum + (a[i] * b[i]);
  }

  return sum;
}

def float vec_dot_productf(float[3] a, float[3] b) {
  float sum;
  int i;
  sum = 0.0;
  i = 0;

  for(i = 0; i < 3; i=i+1) {
    sum = sum + (a[i] * b[i]);
  }

  return sum;
}

def int[3] vec_vec_addi(int[3] a, int[3] b) {
