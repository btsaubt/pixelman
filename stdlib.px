  }
  return a;
}

def int[3][3] mat_mat_add(int[3][3] a, int[3][3] b) {
  int i;
  int j;
  for(i = 0; i < 3; i = i+1) {
    for(j = 0; j < 3; j = j+2) {
      a[i][j] = a[i][j] + b[i][j];
    }
  }
  return a;
}


