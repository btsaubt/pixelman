def int[3][3] mat_mat_multi(int[3][3] a, int[3][3] b) {
  int i;
  int j;
  int[3][3] c;
  for(i = 0; i < 3; i = i+1) {
    for(j = 0; j < 3; j = j+1) {
      c[i][j] = a[i][0] * b[0][j] + a[i][1] * b[1][j] + a[i][2] * b[2][j];
    }
  }
  return c;
}

